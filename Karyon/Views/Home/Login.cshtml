<script>
    function checkMobileNo() {
        var Idtype = $('input[name="Idtype"]:checked').val();
        var termsId = $('input[name="termconId"]:checked').val();
        $('#Idtype').val(Idtype);
        if ($('#MobileNo').val() == "") {
            $("#MobileNo").addClass('errortext');
            $('#MobileNo').focus();
            return false;
        }
        if ($('#termsvalue').val() == "0") {
            if (termsId == '' || termsId == null) {
                alert('Please accept terms and conditions')
                return false;
            }
        }
    }
    function fverifyOTP() {
        if ($('#OTP').val() == "") {
            $("#OTP").addClass('errortext');
            $('#OTP').focus();
            return false;
        }
    }
    function funclogin() {
        if ($('input[name="Login"]:checked').val() != "on") {
            alert("Please Select LoginId");
            return false;
        }
    }
    function fgetLoginId(LoginId, Password) {
        $('#LoginId').val(LoginId);
        $('#ConfirmPassword').val(Password);
    }
    function checktermscondition() {
        var Idtype = $('input[name="IdType"]:checked').val();
        $.ajax({
            url: '/Home/CheckTermsCondition',
            type: 'Post',
            data: { MobileNo: $('#MobileNo').val(), LoginType: Idtype },
            success: function (result) {

                $('#termsvalue').val(result.flag);
                if (result.flag == "1") {
                    $('#divterms').hide();
                }
                else {
                    $('#divterms').show();
                }

            }, error: function () {
                //alert('Some Thing wrong!');


            }
        });
    }
    function isNumberKey(evt) {
        var charCode = (evt.which) ? evt.which : event.keyCode;
        if (charCode != 46 && charCode > 31
            && (charCode < 48 || charCode > 57))
            return false;
    }
    
</script>
