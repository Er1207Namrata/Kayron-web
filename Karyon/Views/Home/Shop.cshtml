                    $('#txtproductdetails').text(response[0].productsDetails);
                    $('#varientdetails').text('');
                    for (var i = 0; i < response[0].varientList.length; i++) {

                        if (i == 0) {
                            var mrp = "₹" + response[0].varientList[i].mrp;
                            var offeredPrice = "₹" + response[0].varientList[i].offeredPrice;
                            if (response[0].varientList[i].customerId != "0") {
                                $('#mrpwithlogin').show();

                                $('#mrpwithlogin').text(mrp);
                                $('#offeredPrice').text(offeredPrice);
                            }
                            else {
                                $('#mrpwithlogin').hide();
                                $('#offeredPrice').text(mrp);
                            }
                            $('#productimage').attr('src', response[0].varientList[i].imageList[0].imageUrl);



                        }

                        if (i == 0) {
                            $('#varientid').text(response[0].varientList[i].varientId);
                            str += "<li class='list-item'><input type='radio' onclick='return getVarientDetails(" + response[0].varientList[i].mrp + "," + response[0].varientList[i].offeredPrice + "," + response[0].varientList[i].customerId + "," + response[0].varientList[i].varientId + ")' checked id='modalWeight" + i + "' name='modalWeight'><label for='modalWeight" + i + "'>" + response[0].varientList[i].size + " " + response[0].varientList[i].unitName + "</label></li>";
                        }
                        else {
                            str += "<li class='list-item'><input type='radio' onclick='return getVarientDetails(" + response[0].varientList[i].mrp + "," + response[0].varientList[i].offeredPrice + "," + response[0].varientList[i].customerId + "," + response[0].varientList[i].varientId + ")' id='modalWeight" + i + "' name='modalWeight'><label for='modalWeight" + i + "'>" + response[0].varientList[i].size + " " + response[0].varientList[i].unitName + "</label></li>";
                        }


                    }
                    $('#varientdetails').append(str);


                    $("#pre-loader").hide();

                }
                else {
                    $("#pre-loader").hide();
                }
            }, error: function() {
                //alert('Some Thing wrong!');
                $("#pre-loader").hide();

            }
        });
    }
</script>
