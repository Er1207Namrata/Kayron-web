<script>
     $(document).ready(function () {
         $('.overlay').hide();
     });
    function GetBiddingData() {
        $.ajax({ 
            url: '/Associate/GetOrderListForQr',
            type: 'GET',
            dataType: 'JSON',
            data: { 'OrderId': $('#MerchantTranId').val() },
            success: function (response) {
                $("#btnPlaceOrder").hide();
                if (response != null) {
                    if (response.status == "Pending") {

                        GetBiddingData();
                    }
                    else if (response.status == "TXN_FAILURE") {
                        alert('Payment Failed please contact admin.');
                    }
                    else {
                        window.location.href = "/Associate/OrderList";
                    }
                }
                else {
                    GetBiddingData();
                }
            },
            error: function () {
                GetBiddingData();
            }
        })
    }

    function changeWalletType() {
        var rdbWallet = $('input[name="rdbWallet"]:checked').val();
        $('#WalletType').val(rdbWallet);
        if(rdbWallet=="UPIId")
        {
            $('#upino').show();
        }
        else
        {
            $('#upino').hide();
        }

    }
    function showalladdress(totalcount) {

        var count = 3;
        for (var i = 0; i <= totalcount - 1; i++) {
            $('#divshowaddress_' + count).show();
            count = count + 1;
        }


        $('#viewallbutton').hide();
    }
    function ckeckProduct() {
        debugger
        if ($('#Pk_AddressId').val() == "0") {
            alert("Please Select Address");
            return false;
        }
        if ($('#CheckName').text() == "") {
            alert("Please Update Address");
            return false;
        }
        if ($('#CheckMobileNo').text() == "") {
            alert("Please Update Address");
            return false;
        }
        if ($('#CheckAddress').text() == "") {
            alert("Please Update Address");
            return false;
        }
        if ($('#CheckLocality').text() == "") {
            alert("Please Update Address");
            return false;
        }
        if ($('#CheckLandmark').text() == "") {
            alert("Please Update Address");
            return false;
        }
        if ($('#CheckPincode').text() == "") {
            alert("Please Update Address");
            return false;
        }
        if ($('#CheckState').text() == "") {
            alert("Please Update Address");
            return false;
        }
        if ($('#CheckCity').text() == "") {
            alert("Please Update Address");
            return false;
        }
        var rdbWallet = $('input[name="rdbWallet"]:checked').val();
        var finalamt = $('#Amount').val();

        if (rdbWallet == "FUPWallet") {
            if (Number($('#FUPBalance').val()) < Number(finalamt)) {
                window.location.href = "/Associate/CheckOut?Status=1" + "&PromoCode=" + $('#PromoCode').val() + "&PromoCodeDiscountPrice=" + $('#PromoDiscountPrice').val();
                return false;
            }
            $('#WalletType').val(rdbWallet);
            window.location.href = "/Home/CartList?WalletType=" + $('#WalletType').val() + "&checkout=1" + "&Pk_AddressId=" + $('#Pk_AddressId').val() + "&PromoCode=" + $('#PromoCode').val() + "&PromoCodeDiscountPrice=" + $('#PromoDiscountPrice').val();
        }
        else if (rdbWallet == "KaryonWallet") {
            if (Number($('#WalletBalance').val()) < Number(finalamt)) {
                window.location.href = "/Associate/CheckOut?Status=1"+ "&PromoCode=" + $('#PromoCode').val() + "&PromoCodeDiscountPrice=" + $('#PromoDiscountPrice').val();
                return false;
            }
            $('#WalletType').val(rdbWallet);
            window.location.href = "/Home/CartList?WalletType=" + $('#WalletType').val() + "&checkout=1" + "&Pk_AddressId=" + $('#Pk_AddressId').val() + "&PromoCode=" + $('#PromoCode').val() + "&PromoCodeDiscountPrice=" + $('#PromoDiscountPrice').val();
        }
        else if (rdbWallet == "UPIId") {
            $.ajax({
                url: '/Home/PayUPIID',
                type: 'Post',
                data: { PayerVPA: $('#txtuppino').val(), amount: $('#Amount').val(), Type: "Place Order", LoginId: $('#LoginId').val(), merchantTranId: $('#MerchantTranId').val(), Pk_AddressId: $('#Pk_AddressId').val() },
                success: function (response) {
                    if (response.success=="true")
                    {
                        window.location.href = "/Home/ProcessingPayment?OrderId=" + response.merchantTranId;
                       // $('#MerchantTranId').val(response.merchantTranId);
                       // GetBiddingData();
                    }
                   else
                   {
                        alert(response.message);
                   }
