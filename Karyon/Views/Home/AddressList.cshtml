    function applyPromoCode() {
        debugger
        if ($('#PromoCode').val() == null || $('#PromoCode').val() == "" || $('#PromoCode').val() == 0) {
            $('#PromoCode').focus()
            $("#PromoCode").css("border-color", "red");
            return false;
        }
        else {
            $("#PromoCode").css("border-color", "green");

        }
        let PromoCode = $('#PromoCode').val();
        let Amount = $('#Amount').val();
        if (PromoCode != '') {
            $.ajax({
                url: '/Home/ApplyPromoCode',
                type: 'Post',
                data: { PromoCode: PromoCode, Amount: Amount },
                success: function (response) {
                    if (response.status == 0) {
                        debugger
                        $('#promocodeMessage').html(response.message)
                        $('#Amount1').val(response.orderAmount)
                    }
                    else {
                        debugger
                        $('#ShowDiscount').show()
                        $('#promocodeMessage').html(response.message)
                        $('#DiscountAmount').html(response.orderAmount)
                        $('#PromoDiscountPrice').val(response.orderAmount)
                        let Total = parseFloat(Amount) - parseFloat(response.orderAmount)
                        $('#Total').html(Total)
                        $('#Amount1').val(Total)
                    }
                }, error: function () {
                }
            });
        }
    }
