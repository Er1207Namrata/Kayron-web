@model Karyon.Models.ProfileRequest

@{
    ViewData["Title"] = "ViewProfile";
    Layout = "~/Views/Shared/_AssociateLayout.cshtml";
}




<div class="page-body">
    <form asp-action="ViewProfile" enctype="multipart/form-data">
        <div class="container-fluid">
            <div class="page-title">
                <div class="row">
                    <div class="col-sm-6 ps-0">
                        <h3>PROFILE</h3>
                    </div>
                    <div class="col-sm-6 pe-0">
                        <ol class="breadcrumb">
                            <li class="breadcrumb-item">
                                <a href="#">
                                    <svg class="stroke-icon">
                                        <use href="../../associatepanel/assets/svg/icon-sprite.svg#stroke-home"></use>
                                    </svg>
                                </a>
                            </li>
                            <li class="breadcrumb-item">My Accounts</li>
                            <li class="breadcrumb-item active"> Profile</li>
                        </ol>
                    </div>
                </div>
            </div>
        </div>

        <!-- /breadcrumb -->

        <div class="row">
            <div class="col-xl-12">
                <div class="card overflow-hidden">
                    <div class="card-body p-0">
                        <div class="px-3 pt-3 pos-relative">
                            <div class="rounded-circle profile-img d-flex justify-content-between">
                                @if (@TempData["ProfilePicURL"] != "https://www.karyon.organic")
                                {
                                    <img src="@TempData["ProfilePicURL"]" alt="img" class="avatar-xxl rounded-circle" style="width:50px;height:50px" />
                                }
                                else
                                {
                                    <img src="~/assets/tree/treeicon/active-member.png" alt="img" class="avatar-xxl rounded-circle" style="width:50px;height:50px">
                                }
                                @if (Model != null)
                                {
                                if (Model.IsBid == "false")
                                {
                                <button class="btn btn-primary becomebid btn-sm" fdprocessedid="8ich8x" onclick="return BecomeBid();">Become a BID</button>
                                 }
                                }

                            </div>
                            <div style="text-align:right;"><b class="text-danger">@TempData["RegistraionMember"] </b></div>
                            <div class="mt-2 d-sm-flex">
                                <div class="mt-0 mb-sm-0 mb-2">
                                    <h3 class="mb-1">@Model.FirstName @Model.MiddleName @Model.LastName</h3>
                                    <span class="tx-muted">@Model.RankName</span>
                                </div>

                                <div class="ms-auto mg-sm-t-0-f">
                                    <div class="d-sm-flex">
                                        <div class="d-flex align-items-center me-2 mb-2 mg-b-sm-0-f">
                                            <span class="avatar rounded-circle bg-primary-transparent"><i class="fe fe-briefcase tx-primary"></i></span>
                                            <div class="ms-2">
                                                <p class="mb-1 font-weight-semibold">Total Orders</p>
                                                <span class="tx-muted">@Model.TotalOrders</span>

                                            </div>
                                        </div>
                                    </div>
                                    
                                </div>
                            </div>
                        </div>

                        <div class="panel tabs-style7 scaleX">
                            <div class="panel-head">
                                <ul class="nav nav-tabs d-block d-sm-flex">
                                    <li class="nav-item"><a class="nav-link tx-14 font-weight-semibold text-sm-center text-start @ViewBag.PersonalDetails" data-bs-toggle="tab" href="#profileTab1">About</a></li>
                                    <li class="nav-item ms-sm-2 ms-0"><a class="nav-link tx-14 font-weight-semibold text-sm-center text-start" data-bs-toggle="tab" href="#profileTab2">Edit Profile</a></li>
                                    <li class="nav-item ms-sm-2 ms-0"><a class="nav-link tx-14 font-weight-semibold text-sm-center text-start @ViewBag.KYCTabStatus" data-bs-toggle="tab" href="#profileTab3">KYC</a></li>
                                    @*<li class="nav-item ms-sm-2 ms-0"><a class="nav-link tx-14 font-weight-semibold text-sm-center text-start" data-bs-toggle="tab" href="#profileTab4">Gallery</a></li>
                                    <li class="nav-item ms-sm-2 ms-0"><a class="nav-link tx-14 font-weight-semibold text-sm-center text-start" data-bs-toggle="tab" href="#profileTab5">Friends</a></li>*@
                                
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-xl-12">
                <div class="panel-body p-0 border-0">
                    <div class="tab-content">
                        <div class="tab-pane @ViewBag.PersonalDetails" id="profileTab1">
                            <div class="card">
                                <div class="card-body p-0 border-0">

                                    <div class="border-top"></div>
                                    <div class="p-4">
                                        <h4 class="tx-15 text-uppercase font-weight-semibold mb-3">Personal Details</h4>
                                        <div class="d-sm-flex">
                                            <div class="d-flex align-items-center me-4 mb-3 mb-sm-0">

                                                <div class="ms-3">
                                                    <p class="mb-1 font-weight-semibold">Login Id</p>
                                                    <span class="tx-muted">@Context.Session.GetString("LoginId")</span>
                                                </div>
                                            </div>
                                            <div class="d-flex align-items-center me-4 mb-3 mb-sm-0">

                                                <div class="ms-3">
                                                    <p class="mb-1 font-weight-semibold">Joining Date</p>
                                                    <span class="tx-muted">@Model.JoiningDate</span>
                                                </div>
                                            </div>
                                            <div class="d-flex align-items-center me-4 mb-3 mb-sm-0">

                                                <div class="ms-3">
                                                    <p class="mb-1 font-weight-semibold">Status</p>
                                                    <span class="tx-muted">@Model.Status</span>
                                                </div>
                                            </div>
                                            @if(Model.SponsorId=="" || Model.SponsorId==null)
                                            {
                                                
                                            }
                                            else
                                            {
                                                <div class="d-flex align-items-center me-4 mb-3 mb-sm-0">

                                                    <div class="ms-3">
                                                        <p class="mb-1 font-weight-semibold">Sponsor Details</p>
                                                        <span class="tx-muted">@Model.SponsorName (@Model.SponsorId)</span>
                                                    </div>
                                                </div>
                                            }
                                        

                                        </div>
                                    </div>
                                    <div class="border-top"></div>
                                    <div class="p-4">
                                        <h4 class="tx-15 text-uppercase font-weight-semibold mb-3">Contact Details</h4>
                                        <div class="d-sm-flex">
                                            <div class="d-flex align-items-center me-4 mb-3 mb-sm-0">
                                                <span class="avatar rounded-circle bg-custom"><i class="icon ion-md-phone-portrait tx-muted op-7"></i></span>
                                                <div class="ms-3">
                                                    <p class="mb-1 font-weight-semibold">Mobile</p>
                                                    <span class="tx-muted" id="txtbidmobileno">+@Model.MobileNo</span>
                                                </div>
                                            </div>
                                            <div class="d-flex align-items-center me-4 mb-3 mb-sm-0">
                                                <span class="avatar rounded-circle bg-custom"><i class="icon ion-logo-slack tx-muted op-7"></i></span>
                                                <div class="ms-3">
                                                    <p class="mb-1 font-weight-semibold">Email</p>
                                                    <span class="tx-muted">@Model.Email</span>
                                                </div>
                                            </div>
                                            <div class="d-flex align-items-center">
                                                <span class="avatar rounded-circle bg-custom"><i class="icon ion-md-locate tx-muted op-7"></i></span>
                                                <div class="ms-3">
                                                    <p class="mb-1 font-weight-semibold">Current Address</p>
                                                    <span class="tx-muted">@Model.Address,@Model.State,@Model.City</span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="border-top"></div>
                                    <div class="p-4">
                                        <h4 class="tx-15 text-uppercase font-weight-semibold mb-3">Bank Details</h4>
                                        <div class="d-md-flex">

                                            <div class="d-flex align-items-center me-4 mb-4 mb-md-0">

                                                <div class="ms-3">
                                                    <p class="mb-1 font-weight-semibold">Pan No</p>
                                                    <a href="javascript:void(0);" class="tx-inverse"> @Model.PanCard</a>
                                                </div>
                                            </div>
                                            <div class="d-flex align-items-center me-4 mb-4 mb-md-0">

                                                <div class="ms-3">
                                                    <p class="mb-1 font-weight-semibold">Bank Holder Name</p>
                                                    <a href="javascript:void(0);" class="tx-inverse"> @Model.HolderName</a>
                                                </div>
                                            </div>
                                            <div class="d-flex align-items-center me-4 mb-4 mb-md-0">

                                                <div class="ms-3">
                                                    <p class="mb-1 font-weight-semibold">Bank Name</p>
                                                    <a href="javascript:void(0);" class="tx-inverse">@Model.BankName</a>
                                                </div>
                                            </div>
                                            <div class="d-flex align-items-center me-4 mb-4 mb-md-0">

                                                <div class="ms-3">
                                                    <p class="mb-1 font-weight-semibold">Acc No</p>
                                                    <a href="javascript:void(0);" class="tx-inverse">@Model.AccNo</a>
                                                </div>
                                            </div>
                                            <div class="d-flex align-items-center me-4 mb-4 mb-md-0">

                                                <div class="ms-3">
                                                    <p class="mb-1 font-weight-semibold">IFSC Code</p>
                                                    <a href="javascript:void(0);" class="tx-inverse">@Model.IFSC</a>
                                                </div>
                                            </div>
                                            <div class="d-flex align-items-center me-4 mb-4 mb-md-0">

                                                <div class="ms-3">
                                                    <p class="mb-1 font-weight-semibold">Branch</p>
                                                    <a href="javascript:void(0);" class="tx-inverse">@Model.Branch</a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="p-4">
                                        <h4 class="tx-15 text-uppercase font-weight-semibold mb-3">GST Details</h4>
                                        <div class="d-md-flex">
                                            <div class="d-flex align-items-center me-4 mb-4 mb-md-0">
                                                <div class="ms-3">
                                                    <p class="mb-1 font-weight-semibold">GST No</p>
                                                    <a href="javascript:void(0);" class="tx-inverse"> @Model.GSTNo</a>
                                                </div>
                                            </div>
                                            <div class="d-flex align-items-center me-4 mb-4 mb-md-0">
                                                <div class="ms-3">
                                                    <p class="mb-1 font-weight-semibold">Firm Name</p>
                                                    <a href="javascript:void(0);" class="tx-inverse"> @Model.FirmName</a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="border-top"></div>
                                    <div class="p-4">
                                        <h4 class="tx-15 text-uppercase font-weight-semibold mb-3">KYC Details</h4>
                                        <div class="d-md-flex">

                                            <div class="d-flex align-items-center me-4 mb-4 mb-md-0">

                                                <div class="ms-3">
                                                    <p class="mb-1 font-weight-semibold">Pan</p>
                                                    @if (Model.PanStatus == "Pending")
                                                    {
                                                        <span class="badge badge-lg badge-info">@Model.PanStatus</span>
                                                    }
                                                    else if (Model.PanStatus == "Approved")
                                                    {
                                                        <span class="badge badge-lg badge-success">@Model.PanStatus</span>
                                                    }
                                                    else

                                                    {
                                                        <span class="badge badge-lg badge-danger">@Model.PanStatus</span>
                                                    }

                                                </div>
                                            </div>
                                            <div class="d-flex align-items-center me-4 mb-4 mb-md-0">

                                                <div class="ms-3">
                                                    <p class="mb-1 font-weight-semibold">Address Proof Front</p>
                                                    @if (Model.AddressFrontStatus == "Pending")
                                                    {
                                                        <span class="badge badge-lg badge-info">@Model.AddressFrontStatus</span>
                                                    }
                                                    else if (Model.AddressFrontStatus == "Approved")
                                                    {
                                                        <span class="badge badge-lg badge-success">@Model.AddressFrontStatus</span>
                                                    }
                                                    else

                                                    {
                                                        <span class="badge badge-lg badge-danger">@Model.AddressFrontStatus</span>
                                                    }
                                                </div>
                                            </div>
                                            <div class="d-flex align-items-center me-4 mb-4 mb-md-0">

                                                <div class="ms-3">
                                                    <p class="mb-1 font-weight-semibold">Address Proof Back</p>
                                                    @if (Model.AddressBackStatus == "Pending")
                                                    {
                                                        <span class="badge badge-lg badge-info">@Model.AddressBackStatus</span>
                                                    }
                                                    else if (Model.AddressBackStatus == "Approved")
                                                    {
                                                        <span class="badge badge-lg badge-success">@Model.AddressBackStatus</span>
                                                    }
                                                    else

                                                    {
                                                        <span class="badge badge-lg badge-danger">@Model.AddressBackStatus</span>
                                                    }
                                                </div>
                                            </div>
                                            <div class="d-flex align-items-center me-4 mb-4 mb-md-0">

                                                <div class="ms-3">
                                                    <p class="mb-1 font-weight-semibold">Bank</p>
                                                    @if (Model.BankStatus == "Pending")
                                                    {
                                                        <span class="badge badge-lg badge-info">@Model.BankStatus</span>
                                                    }
                                                    else if (Model.BankStatus == "Approved")
                                                    {
                                                        <span class="badge badge-lg badge-success">@Model.BankStatus</span>
                                                    }
                                                    else

                                                    {
                                                        <span class="badge badge-lg badge-danger">@Model.BankStatus</span>
                                                    }
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="tab-pane" id="profileTab2">

                            <div class="card">
                                <div class="card-body border-0">
                                    <div class="d-flex">
                                        <div class="ms-auto">
                                            <input type="submit" id="btnEditProfile" name="btnUpdate" class="btn btn-sm btn-primary rounded-pill" value="Update" onclick="return Fvalidate();" />
                                            @* <a href="javascript:void(0);" class="btn btn-sm btn-primary rounded-pill">Save Changes</a>
                                            <a href="javascript:void(0);" class="btn btn-sm btn-outline-danger rounded-pill">Cancel</a>*@
                                        </div>
                                    </div>

                                    <h4 class="tx-15 text-uppercase font-weight-semibold mb-3">Contact Details</h4>
                                    <div class="form-group  mb-3">
                                        <div class="row row-sm">
                                            <div class="col-md-3">
                                                <label class="form-label">Email</label>
                                            </div>
                                            <div class="col-md-9">
                                                <input type="text" class="form-control" asp-for="Email" placeholder="Email" maxlength="50" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group  mb-3">
                                        <div class="row row-sm">
                                            <div class="col-md-3">
                                                <label class="form-label">Address</label>
                                            </div>
                                            <div class="col-md-9">
                                                <textarea class="form-control" type="text" asp-for="Address" maxlength="100" placeholder="Address"></textarea>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group  mb-3">
                                        <div class="row row-sm">
                                            <div class="col-md-3">
                                                <label class="form-label">Pincode</label>
                                            </div>
                                            <div class="col-md-9">
                                                <input type="number" placeholder="Pincode" asp-for="Pincode" onchange=" GetStateCity()" min="1" class="form-control" maxlength="6" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group  mb-3">
                                        <div class="row row-sm">
                                            <div class="col-md-3">
                                                <label class="form-label">City</label>
                                            </div>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="City" asp-for="City" readonly class="form-control" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group  mb-3">
                                        <div class="row row-sm">
                                            <div class="col-md-3">
                                                <label class="form-label">State</label>
                                            </div>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="State" asp-for="State" readonly class="form-control" />

                                            </div>
                                        </div>
                                    </div>


                                    <h4 class="tx-15 text-uppercase font-weight-semibold mb-3">Bank Details</h4>
                                    <div class="form-group  mb-3">
                                        <div class="row row-sm">
                                            <div class="col-md-3">
                                                <label class="form-label">A/C No</label>
                                            </div>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="Account No" asp-for="AccNo" min="1" class="form-control" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group  mb-3">
                                        <div class="row row-sm">
                                            <div class="col-md-3">
                                                <label class="form-label">Bank Holder</label>
                                            </div>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="Holder Name" asp-for="HolderName" min="1" class="form-control" maxlength="50" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group  mb-3">
                                        <div class="row row-sm">
                                            <div class="col-md-3">
                                                <label class="form-label">Pan Card</label>
                                            </div>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="PanCard" asp-for="PanCard" min="10" class="form-control" maxlength="10" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group  mb-3">
                                        <div class="row row-sm">
                                            <div class="col-md-3">
                                                <label class="form-label">Bank Name</label>
                                            </div>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="BankName" asp-for="BankName" min="1" class="form-control" maxlength="50" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group  mb-3">
                                        <div class="row row-sm">
                                            <div class="col-md-3">
                                                <label class="form-label">IFSC</label>
                                            </div>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="IFSC" asp-for="IFSC" class="form-control" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group  mb-3">
                                        <div class="row row-sm">
                                            <div class="col-md-3">
                                                <label class="form-label">Branch</label>
                                            </div>
                                            <div class="col-md-9">
                                                <input type="Text" placeholder="Branch Name" asp-for="Branch" class="form-control" />
                                            </div>
                                        </div>
                                    </div>
                                    <h4 class="tx-15 text-uppercase font-weight-semibold mb-3">GST Details</h4>
                                    <div class="form-group  mb-3">
                                        <div class="row row-sm">
                                            <div class="col-md-3">
                                                <label class="form-label">GSt No</label>
                                            </div>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="Gst No" asp-for="GSTNo" class="form-control" onchange="return validategst();" />
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group  mb-3">
                                        <div class="row row-sm">
                                            <div class="col-md-3">
                                                <label class="form-label">Firm Name</label>
                                            </div>
                                            <div class="col-md-9">
                                                <input type="text" placeholder="Firm Name" asp-for="FirmName" class="form-control" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="tab-pane @ViewBag.Tab3Status" id="profileTab3">
                            <div class="row row-sm">
                                <div class="col-12">
                                    <div class="card">
                                        <div class="card-body border-0">
                                            <div class="d-flex">
                                                <div class="ms-auto">

                                                    @* <a href="javascript:void(0);" class="btn btn-sm btn-primary rounded-pill">Save Changes</a>
                                                    <a href="javascript:void(0);" class="btn btn-sm btn-outline-danger rounded-pill">Cancel</a>*@
                                                </div>
                                            </div>

                                            <h4 class="tx-15 text-uppercase font-weight-semibold mb-3">KYC Details</h4>
