<script>
    $(document).ready(function () {
        var Gender = $('input[name="Gender"]:checked').val();
        $('#Gender').val(Gender);
        $("#pre-loader").show();
    })
    function BecomeBid() {
        var status = confirm("Are you sur you want to become a BID");
        if (status == true) {
            var mobileno = $('#txtbidmobileno').text();
            $("#pre-loader").show();
            $.ajax({
                url: '/Associate/UpdateBID',
                type: 'Post',
                data: { mobileno: mobileno },
                success: function (response) {
                    if (response != null) {
                        //console.log(response);
                        if (response.flag == 1) {
                            alert(response.status);
                            $('.becomebid').hide();
                        }
                        else {

                            alert(response.status);

                        }

                        $("#pre-loader").hide();

                    }
                    else {
                        $("#pre-loader").hide();
                    }
                }, error: function () {

                    $("#pre-loader").hide();

                }
            });
        }

    }
    function GetStateCity() {
        $(".errortext").removeClass("errortext");
        $("#pre-loader").show();
        $.ajax({
            url: '/Home/GetStateCity',
            type: 'Post',
            data: { PinCode: $('#Pincode').val() },
            success: function (response) {
                if (response != null) {
                    console.log(response);
                    if (response.status == 1) {
                        $('#State').val(response.response.state);
                        $('#City').val(response.response.city);
                    }
                    else {
                        $('#Pincode').val('');
                        $('#State').val('');
                        $('#City').val('');
                        $("#Pincode").addClass('errortext');
                        $('#Pincode').focus();
                        alert(response.message);

                    }

                    $("#pre-loader").hide();

                }
                else {
                    $("#pre-loader").hide();
                }
            }, error: function () {

                $("#pre-loader").hide();

            }
        });
    }

    function validategst()
    {
