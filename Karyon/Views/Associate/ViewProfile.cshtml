                                                        <th>
                                                            <label class="form-label">Pan Image</label>
                                                        </th>
                                                        <th>
                                                            <input type="file" asp-for="ImageFile" />
                                                        </th>
                                                        <th>
                                                            <img src="@TempData["PanImage"]" style="width:50px;" />
                                                        </th>


                                                        <th>  <input type="submit" class="btn btn-success" name="btnpanimage" id="btnpanimage" /></th>

                                                    </tr>
                                                    <tr>
                                                        <th>
                                                            <label class="form-label">Address Proof Front</label>
                                                        </th>
                                                        <th>
                                                            <input type="file" asp-for="AddressProofFront" />
                                                        </th>
                                                        <th>
                                                            <img src="@TempData["AddressProofFrontURL"]" style="width:50px;" />
                                                        </th>


                                                        <th>   <input type="submit" class="btn btn-success" name="btnaddressfront" id="btnaddressfront" /></th>

                                                    </tr>
                                                    <tr>
                                                        <th>
                                                            <label class="form-label">Address Proof Back</label>
                                                        </th>
                                                        <th>
                                                            <input type="file" asp-for="AddressProofBack" />
                                                        </th>
                                                        <th>
                                                            <img src="@TempData["AddressProofBackURL"]" style="width:50px;" />
                                                        </th>


                                                        <th>
                                                            <input type="submit" class="btn btn-success" name="btnaddressback" id="btnaddressback" />
                                                        </th>

                                                    </tr>
                                                    <tr>
                                                        <th>
                                                            <label class="form-label">Bank Doc</label>
                                                        </th>
                                                        <th>
                                                            <input type="file" asp-for="BankDoc" />
                                                        </th>
                                                        <th>
                                                            <img src="@TempData["BankDocURL"]" style="width:50px;" />
                                                        </th>


                                                        <th>
                                                            <input type="submit" class="btn btn-success" name="btnbank" id="btnbank" />
                                                        </th>

                                                    </tr>
                                                </thead>
                                            </table>

                                           
                                            
                                          
                                           
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>
                        <div class="tab-pane" id="profileTab4">
                            <div class="masonry row">
                                <div class="col-xl-3 col-lg-4 col-sm-6">
                                    <div class="brick">
                                        <a href="../assets/img/photos/1.jpg" class="js-img-viewer"
                                           data-caption="IMAGE-01" data-id="lion">
                                            <img src="../assets/img/photos/1.jpg" alt="" />
                                        </a>
                                    </div>
                                </div>
                                <div class="col-xl-3 col-lg-4 col-sm-6">
                                    <div class="brick">
                                        <a href="../assets/img/photos/2.jpg" class="js-img-viewer"
                                           data-caption="IMAGE-02" data-id="camel">
                                            <img src="../assets/img/photos/2.jpg" alt="" />
                                        </a>
