<script>

    function fvalidate() {
        if ($('#Email').val() == "") {
            $('#Email').focus();
            alert("Please Add Email!");
            return false;
        }
        if ($('#Address').val() == "") {
            $('#Address').focus();
            alert("Please Add Address!");
            return false;
        }
        if ($('#Pincode').val() == "") {
            $('#Pincode').focus();
            alert("Please Add Pincode!");
            return false;
        }
        if ($('#AccNo').val() == "") {
            $('#AccNo').focus();
            alert("Please Add Account Number!");
            return false;
        }
        if ($('#HolderName').val() == "") {
            $('#HolderName').focus();
            alert("Please Add Holder's Name!");
            return false;
        }
        if ($('#PanCard').val() == "") {
            $('#PanCard').focus();
            alert("Please Add PanCard!");
            return false;
        }
        if ($('#BankName').val() == "") {
            $('#BankName').focus();
            alert("Please Add BankName!");
            return false;
        }
        if ($('#IFSC').val() == "") {
            $('#IFSC').focus();
            alert("Please Add IFSC Code!");
            return false;
        }
        if ($('#Branch').val() == "") {
            $('#Branch').focus();
            alert("Please Add Branch!");
            return false;
        }
        
    }
    function GetStateCity() {
        $(".errortext").removeClass("errortext");
        $("#pre-loader").show();
        $.ajax({
            url: '/Home/GetStateCity',
            type: 'Post',
            data: { PinCode: $('#Pincode').val() },
            success: function (response) {
                if (response != null) {
                    console.log(response);
                    if (response.status == 1) {
                        $('#State').val(response.response.state);
                        $('#City').val(response.response.city);
                    }
                    else {
                        $('#Pincode').val('');
                        $('#State').val('');
                        $('#City').val('');
                        $("#Pincode").addClass('errortext');
                        $('#Pincode').focus();
                        alert(response.message);

                    }

                    $("#pre-loader").hide();

                }
                else {
                    $("#pre-loader").hide();
                }
            }, error: function () {

                $("#pre-loader").hide();

            }
        });
    }
    function ValidatePAN() {
        var PanNo = $('#PanCard').val()
        if (PanNo.value != "") {
            var panPattern = /^([a-zA-Z]{5})(\d{4})([a-zA-Z]{1})$/;
            if (PanNo.search(panPattern) == -1) {
                alert("Please enter valid pan number");
                $('#PanCard').val('');
                return false;
            }

        }
    }
</script>











